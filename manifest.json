{
    "manifest_version": 2,
    "name": "InstagramPhotoLink",
    "description": "This extension adds a link to the actual image (and some videos) in instagram.",
    "version": "2.5",
    "content_scripts": [
        {
            "matches": [ "https://www.instagram.com/*" ],
            "js": [ "findlink.js" ],
            "run_at": "document_idle"
        } 
    ],
    "icons": { "128": "icon.png","48": "icon.png","16": "icon.png" }
}
